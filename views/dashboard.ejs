            crossorigin="anonymous"></script>

    <script src="../angular/angular.js"></script>
    <script src="../angular-resource/angular-resource.js"></script>
    <script src="../angular-ui-router/release/angular-ui-router.js"></script>
    <script src="../app.js"></script>

    <!--services-->
    <script src="../common/services/user.service.js"></script>

    <!--components-->
    <script src="../common/components/navBar/navBar.component.js"></script>
    <script src="../common/components/sideBar/sideBar.component.js"></script>

    <script src="../profile/components/profile.component.js"></script>
    <script src="../users/components/users.component.js"></script>
</body>
</html>