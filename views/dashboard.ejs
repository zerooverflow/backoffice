    <link rel="stylesheet" href="../css/style.css">
</head>
<body>
    <div class="container-fluid">
        <nav-bar username="<%= user.name %>"></nav-bar>
        <div class="admin-content row">
            <aside class="main-sidebar col-sm-1">
                <side-bar></side-bar>
            </aside>
            <ui-view class="col-sm-offset-1 col-sm-11 col-xs-offset-1 col-xs-10"></ui-view>
        </div>
